    },
    "nanocell_particles_on_damage": {
        "type": "origins:action_when_damage_taken",
        "entity_action": {
            "type": "origins:spawn_particles",
            "particle": "minecraft:enchanted_hit",
            "count": 32,
            "speed": 1,
            "spread": {
                "x": 0.0,
                "y": 0.1,
                "z": 0.0
            }
        },
        "cooldown": 1
    },
