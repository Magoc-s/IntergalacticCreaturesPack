{
    "name": "Nano-cell Barrier",
    "description": "Instead of taking damage, you are able to use excess nano-cells to absorb damage. Use your [secondary active] key to toggle barrier regeneration. Damage over 20 cannot be mitigated.",
    "type": "origins:multiple",    
    "nanites_resource": {
        "type": "origins:resource",
        "min": 0,
        "max": 100,
        "name": "Nanites",
        "description": "Stored nanites",
        "hud_render": {
            "should_render": true, 
            "sprite_location": "origins:textures/gui/community/huang/resource_bar_02.png",
            "bar_index": 14,
            "condition": {
                "type": "origins:resource",
                "resource": "*:*_nanites_resource",
                "comparison": "<",
                "compare_to": 100
            }
        }
    },
    "recharge_nanites_toggle": {
        "type": "origins:toggle",
        "active_by_default": false,
        "key": {
            "key": "key.origins.secondary_active"
        }
    },
    "consume_energy_for_nanites": {
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "*:*_nanites_resource",
                    "change": 1
                },
                {
                    "type": "origins:change_resource",
                    "resource": "*:replicator/energy_resource",
                    "change": -1
                }
            ]
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:power_active",
                    "power": "*:*_recharge_nanites_toggle"
                },
                {
                    "type": "origins:resource",
                    "resource": "*:replicator/energy_resource",
                    "comparison": ">",
                    "compare_to": 0
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_nanites_resource",
                    "comparison": "<",
                    "compare_to": 100
                }
            ]
        }
    }, 
